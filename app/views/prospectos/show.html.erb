<%- model_class = Prospecto -%>

<div class="page-header">

  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>

</div>

<dl class="dl-horizontal">

  <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>

  <dd><%= @prospecto.nome %></dd>

  <dt><strong><%= model_class.human_attribute_name(:empresa) %>:</strong></dt>

  <dd><%= @prospecto.empresa %></dd>

  <dt><strong><%= model_class.human_attribute_name(:telefone) %>:</strong></dt>

  <dd><%= @prospecto.telefone %></dd>

  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>

  <dd><%= @prospecto.email %></dd>

  <dt><strong><%= model_class.human_attribute_name(:cidade) %>:</strong></dt>

  <dd><%= @prospecto.cidade %></dd>

  <dt><strong><%= model_class.human_attribute_name(:estado) %>:</strong></dt>

  <dd><%= @prospecto.estado %></dd>

</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),

              prospectos_path, :class => 'btn btn-default'  %>

<%= link_to t('.edit', :default => t("helpers.links.edit")),

              edit_prospecto_path(@prospecto), :class => 'btn btn-default' %>

<%= link_to t('.destroy', :default => t("helpers.links.destroy")),

              prospecto_path(@prospecto),

              :method => 'delete',

              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },

              :class => 'btn btn-danger' %>
